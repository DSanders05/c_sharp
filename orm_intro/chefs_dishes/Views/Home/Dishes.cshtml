<div class="text-center">
    <h1><a href="/">Chefs</a> | Dishes</h1>
    <a href="dishes/new">Add a Dish</a>
    <h3>Yum, take a look at our tasty dishes!</h3>
    <hr>
    <table class="table table-striped">
        <tr>
            <th>Name of Dish</th>
            <th>Chef</th>
            <th>Tastiness</th>
            <th>Calories</th>
        </tr>
        @{
            foreach(Dish d in ViewBag.allDishes)
            {
                <tr>
                    <td>@d.Name</td>
                    <td>@d.Creator.FirstName @d.Creator.LastName</td>
                    <td>@d.Tastiness</td>
                    <td>@d.Description</td>
                </tr>
            }
        }
    </table>
</div>